generator client {
  provider = "prisma-client"
  output   = "../src/app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Question {
  id              String @id
  title           String
  difficulty      Int
  type            String
  votes           Int    @default(0)
  questionSummary String @map("question_summary")
  companyAsked    String @map("company_asked")

  @@index([difficulty, type])
  @@map("questions")
}
